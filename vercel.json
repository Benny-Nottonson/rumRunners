{
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    }
  ],
  "chrons": [
    {
      "path": "#!/usr/bin/env node && src/init.mjs",
      "schedule": "0 5 * * *"
    }
  ]
}
